<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ThemeSelector.Resources.Themes.Dark">

    <Color x:Key="PrimaryBackground">#212121</Color>
    <Color x:Key="PrimaryForeground">WhiteSmoke</Color>
    <Color x:Key="DisabledForeground">#ACACAC</Color>

    <Color x:Key="RadioSelected">FireBrick</Color>
    <Color x:Key="RadioUnselected">WhiteSmoke</Color>


</ResourceDictionary>